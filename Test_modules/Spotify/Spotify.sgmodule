#!name=Spotify Premium Unlock
#!desc=Mở khóa tính năng Premium cho Spotify (Crack By NgDanhThanhTrung).

[Header Rewrite]
^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com)\/user-customization-service\/v1\/customize header-del if-none-match

[Script]
spotify-json = type=http-request, pattern=^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com)\/(artistview\/v1\/artist|album-entity-view\/v2\/album)\/, script-path=https://raw.githubusercontent.com/NgDanhThanhTrung/modules/main/Test_modules/Spotify/JS/spotify-json.js, requires-body=0
spotify-proto = type=http-response, pattern=^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com)\/(bootstrap\/v1\/bootstrap|user-customization-service\/v1\/customize)$, script-path=https://raw.githubusercontent.com/NgDanhThanhTrung/modules/main/Test_modules/Spotify/JS/spotify-proto.js, requires-body=1, binary-body-mode=1
spotify-header = type=http-request, pattern=^https:\/\/(spclient\.wg\.spotify\.com|.*-spclient\.spotify\.com)\/user-customization-service\/v1\/customize, script-path=hhttps://raw.githubusercontent.com/NgDanhThanhTrung/modules/main/Test_modules/Spotify/JS/spotify-qx-header.js, requires-body=0

[MITM]
hostname = %APPEND% spclient.wg.spotify.com, *-spclient.spotify.com, *.googleusercontent.com
